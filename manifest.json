{
    "manifest_version": 3,
    "name": "NCDownloader",
    "description": "NCDownloader extension for Chrome. PLEASE log in to use context menu and url download.",
    "version": "0.1.1",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "contextMenus"
    ],
    "optional_host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "icons": {
        "48": "icon.png"
    }
}